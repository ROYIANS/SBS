/**book.wxss**/
@import "/colorui/icon.wxss";
@import "/colorui/main.wxss";
.page {
    display: block;
    min-height: 100%;
    height: 100vh;
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: -1;
    background-color: #fff;
}

.search {
    background-color: #212121!important;
}

.search-form {
    background-color: #414141!important;
    color: #ccc!important;
}

.cu-bar.fixed {
    box-shadow: 0 6.25rpx 12.5rpx rgba(0, 0, 0, 0.16);
}